    "def line_count(filename):\n",
    "    \"\"\"A function counts how many lines a file has.\"\"\"\n",
    "    cnt = 0\n",
    "    for line in open(filename):\n",
    "        cnt += 1\n",
    "    return cnt\n",
    "\n",
    "print(__name__)\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    print(line_count('wc.py'))"
